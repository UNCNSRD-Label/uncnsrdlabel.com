{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [".env.local"],
  "pipeline": {
    "analyze": {
      "outputs": ["dist/**"]
    },
    "build": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "clean": {
      "cache": false
    },
    "dependencies:check": {
      "cache": false
    },
    "dependencies:graph": {
      "cache": false
    },
    "dependencies:unused": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "dependsOn": ["^build"],
      "env": [
        "ANALYZE",
        "NEXT_PUBLIC_DEFAULT_LOCALE",
        "NEXT_PUBLIC_FEATURE_FLAG_HOLDING_REDIRECT_ENABLE",
        "NEXT_PUBLIC_FEATURE_FLAG_PWA",
        "NEXT_PUBLIC_FEATURE_FLAG_SEARCH_COLLECTIONS_ENABLE",
        "NEXT_PUBLIC_FEATURE_FLAG_WISHLIST_ENABLE",
        "NEXT_PUBLIC_GOOGLE_TAG_MANAGER_ID",
        "NEXT_PUBLIC_HYGRAPH_API_TOKEN",
        "NEXT_PUBLIC_HYGRAPH_PROJECT_ID",
        "NEXT_PUBLIC_HYGRAPH_REGION",
        "NEXT_PUBLIC_SHOPIFY_STOREFRONT_ACCESS_TOKEN",
        "NEXT_PUBLIC_SHOPIFY_STORE_DOMAIN",
        "NEXT_PUBLIC_SHOPIFY_STOREFRONT_API_VERSION",
        "NEXT_PUBLIC_VERCEL_URL",
        "NODE_ENV",
        "SITE_NAME",
        "SITE_URL_WEB"
      ],
      "outputs": ["dist/**"]
    },
    "generate-types": {
      "outputs": []
    },
    "lint": {
      "dependsOn": ["^build"]
    },
    "start": {
      "cache": false,
      "dependsOn": ["^build"],
      "outputs": []
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "types:check": {
      "outputs": []
    }
  }
}
