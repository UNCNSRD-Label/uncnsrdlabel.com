{
  "name": "uncnsrdlabel.com",
  "version": "0.1.0",
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "dev": "next dev",
    "find:unused": "npx next-unused",
    "lint": "next lint",
    "lint-staged": "lint-staged",
    "prettier:check": "prettier --check --ignore-unknown .",
    "prettier:fix": "prettier --write --ignore-unknown .",
    "start": "next start",
    "test": "npm prettier:check && npm lint"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "clsx": "^1.2.1",
    "cross-fetch": "^3.1.5",
    "next": "^13.4.1",
    "open-props": "^1.5.8",
    "postcss-jit-props": "^1.0.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-player": "^2.12.0",
    "react-use": "^17.4.0",
    "react-use-wizard": "^2.2.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.4.1",
    "@types/js-cookie": "^3.0.3",
    "@types/node": "^20.1.0",
    "@types/react": "^18.2.5",
    "@vercel/git-hooks": "^1.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.40.0",
    "eslint-config-next": "^13.4.1",
    "eslint-config-prettier": "^8.8.0",
    "lint-staged": "^13.2.2",
    "next-compose-plugins": "^2.2.1",
    "next-pwa": "^5.6.0",
    "postcss": "^8.4.23",
    "postcss-easings": "^3.0.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-nesting": "^11.2.2",
    "postcss-preset-env": "^8.3.2",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4"
  },
  "git": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write --ignore-unknown"
    ],
    "**/*.{md,mdx,json}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  },
  "packageManager": "pnpm@8.4.0"
}
