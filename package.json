{
  "name": "uncnsrdlabel.com",
  "version": "0.1.0",
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "codegen": "graphql-codegen --config codegen.ts --watch",
    "dev": "run-p dev:next codegen",
    "dev:next": "next dev",
    "find:unused": "npx next-unused",
    "lint": "next lint",
    "lint-staged": "lint-staged",
    "prettier:check": "prettier --check --ignore-unknown .",
    "prettier:fix": "prettier --write --ignore-unknown .",
    "start": "next start",
    "test": "npm prettier:check && npm lint",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@graphql-typed-document-node/core": "^3.1.1",
    "@heroicons/react": "^2.0.13",
    "@next/font": "^13.0.6",
    "@react-three/drei": "^9.46.0",
    "@react-three/fiber": "^8.9.1",
    "@react-three/postprocessing": "^2.7.0",
    "@shopify/hydrogen-react": "2022.10.4",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.8",
    "clsx": "^1.2.1",
    "cross-fetch": "^3.1.5",
    "daisyui": "^2.42.1",
    "dinero.js": "^2.0.0-alpha.8",
    "email-validator": "^2.0.4",
    "encoding": "^0.1.13",
    "glsl-random": "^0.0.5",
    "graphql-request": "^5.0.0",
    "js-cookie": "^3.0.1",
    "keen-slider": "^6.8.5",
    "next": "^13.0.6",
    "next-pwa": "^5.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-scroll": "^1.8.8",
    "react-social-icons": "^5.15.0",
    "react-use-wizard": "^2.2.1",
    "tailwindcss-colors-css-variables": "^1.1.2-rc.2",
    "three": "^0.147.0",
    "three-stdlib": "^2.20.2",
    "timeline-composer": "^0.1.7"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.15.0",
    "@graphql-codegen/client-preset": "1.2.1",
    "@next/bundle-analyzer": "^13.0.6",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/js-cookie": "^3.0.2",
    "@types/node": "^18.11.10",
    "@types/react": "^18.0.26",
    "@types/react-dom": "18.0.9",
    "@types/react-scroll": "^1.8.5",
    "@types/three": "^0.146.0",
    "@vercel/git-hooks": "^1.0.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.28.0",
    "eslint-config-next": "^13.0.6",
    "eslint-config-prettier": "^8.5.0",
    "glslify": "^7.1.1",
    "glslify-loader": "^2.0.0",
    "graphql": "^16.6.0",
    "graphql-config": "^4.3.6",
    "lint-staged": "^13.0.4",
    "next-compose-plugins": "^2.2.1",
    "next-pwa": "^5.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-nesting": "^10.2.0",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.8.0",
    "prettier-plugin-tailwindcss": "^0.2.0",
    "tailwindcss": "^3.2.4",
    "type-fest": "^3.3.0",
    "typescript": "4.9.3"
  },
  "git": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write --ignore-unknown"
    ],
    "**/*.{md,mdx,json}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  },
  "packageManager": "pnpm@7.17.1"
}
