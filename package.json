{
  "devDependencies": {
    "@turbo/gen": "^1.12.4",
    "@types/node": "^20.11.20",
    "dotenv-cli": "^7.3.0",
    "eslint-config-turbo": "^1.12.4",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "madge": "^6.1.0",
    "turbo": "^1.12.4"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npm lint"
    ]
  },
  "name": "uncnsrdlabel.com",
  "packageManager": "pnpm@8.14.0",
  "private": true,
  "repository": "https://github.com/UNCNSRD-Label/uncnsrdlabel.com",
  "scripts": {
    "analyze": "dotenv -c -- turbo run analyze",
    "build": "dotenv -c -- turbo run build",
    "clean": "turbo run clean",
    "dependencies:check": "turbo run dependencies:check",
    "dependencies:graph": "turbo run dependencies:graph",
    "dependencies:unused": "turbo run dependencies:unused",
    "dev": "dotenv -c -- turbo dev --concurrency=12",
    "find:unused": "npx next-unused",
    "lint-staged": "lint-staged",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "list:projects": "pnpm m ls --depth -1 --json",
    "list:projects:dependencies": "pnpm recursive list --only-projects",
    "packages:versions:check": "npx npm-check-updates -u --deep",
    "packages:versions:update": "npx npm-check-updates -u --deep",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "shadcn:add": "pnpm dlx shadcn-ui@latest add --cwd packages/@uncnsrdlabel/components --path src/atoms",
    "start": "turbo run start",
    "test:e2e": "playwright test",
    "test": "echo \"Error: no test specified\" && exit 0",
    "types:check": "turbo run types:check",
    "unused:fix": "npx next-unused --fix",
    "unused": "npx next-unused",
    "update": "turbo run update",
    "validate": "pnpm test && pnpm types:check && pnpm build && pnpm unused"
  }
}
