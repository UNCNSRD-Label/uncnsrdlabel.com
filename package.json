{
  "name": "uncnsrdswim.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "dev": "next dev",
    "find:unused": "npx next-unused",
    "lint": "next lint",
    "prettier-fix": "prettier --write .",
    "start": "next start"
  },
  "dependencies": {
    "clsx": "1.2.1",
    "cross-fetch": "^3.1.5",
    "email-validator": "^2.0.4",
    "js-cookie": "^3.0.1",
    "keen-slider": "^6.7.0",
    "next": "12.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-use-wizard": "2.2.1",
    "tailwindcss-colors-css-variables": "1.1.2-rc.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.2.5",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/js-cookie": "^3.0.2",
    "@types/node": "18.7.9",
    "@types/react": "18.0.17",
    "autoprefixer": "^10.4.8",
    "eslint": "^8.23.0",
    "eslint-config-next": "^12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "lint-staged": "^13.0.3",
    "next-compose-plugins": "^2.2.1",
    "next-pwa": "^5.6.0",
    "postcss": "^8.4.16",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-nesting": "^10.1.10",
    "postcss-preset-env": "^7.8.0",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "**/*.{md,mdx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  }
}
