{
  "name": "uncnsrdlabel.com",
  "version": "0.1.0",
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "codegen": "graphql-codegen --config codegen.ts --watch",
    "dev": "run-p dev:next codegen",
    "dev:next": "next dev",
    "dev:next:inspect": "NODE_OPTIONS='--inspect' next dev",
    "find:unused": "npx next-unused",
    "lint": "next lint",
    "lint-staged": "lint-staged",
    "postbuild": "next-sitemap",
    "prettier:check": "prettier --check --ignore-unknown .",
    "prettier:fix": "prettier --write --ignore-unknown .",
    "prettier:list-different": "npx prettier . --list-different",
    "start": "next start",
    "test": "npm prettier:check && npm lint",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@graphql-typed-document-node/core": "^3.1.1",
    "@headlessui/react": "^1.7.7",
    "@heroicons/react": "^2.0.13",
    "@next/font": "^13.1.1",
    "@react-three/drei": "^9.51.2",
    "@react-three/fiber": "^8.10.0",
    "@react-three/postprocessing": "^2.7.0",
    "@shopify/hydrogen": "^1.6.5",
    "@shopify/hydrogen-react": "2022.10.6",
    "clsx": "^1.2.1",
    "cookies-next": "^2.1.1",
    "cross-fetch": "^3.1.5",
    "dinero.js": "^2.0.0-alpha.8",
    "encoding": "^0.1.13",
    "glsl-random": "^0.0.5",
    "graphql-request": "^5.1.0",
    "js-cookie": "^3.0.1",
    "keen-slider": "^6.8.5",
    "next": "^13.1.1",
    "next-pwa": "^5.6.0",
    "next-query-params": "^4.1.0",
    "next-seo": "^5.15.0",
    "next-sitemap": "^3.1.44",
    "react": "^18.2.0",
    "react-barcode": "^1.4.6",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.41.5",
    "react-icons": "^4.7.1",
    "react-merge-refs": "^2.0.1",
    "react-schemaorg": "^2.0.0",
    "react-social-icons": "^5.15.0",
    "react-use-scrollspy": "^3.1.1",
    "react-world-flags": "^1.5.1",
    "schema-dts": "^1.1.0",
    "sharp": "^0.31.3",
    "slugify": "^1.6.5",
    "tailwindcss-colors-css-variables": "^1.1.2-rc.2",
    "three": "^0.148.0",
    "three-stdlib": "^2.21.5",
    "timeline-composer": "^0.1.7",
    "use-query-params": "^2.1.2",
    "valtio": "^1.8.0"
