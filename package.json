{
  "devDependencies": {
    "@graphql-eslint/eslint-plugin": "^3.20.1",
    "@turbo/gen": "^1.10.16",
    "@types/node": "^20.10.2",
    "@uncnsrdlabel/eslint-config-custom": "workspace:*",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.55.0",
    "eslint-config-turbo": "^1.10.16",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "madge": "^6.1.0",
    "prettier": "^3.1.0",
    "turbo": "^1.10.16"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "lint"
    ]
  },
  "name": "uncnsrdlabel.com",
  "packageManager": "pnpm@8.6.10",
  "private": true,
  "repository": "https://github.com/UNCNSRD-Label/uncnsrdlabel.com",
  "scripts": {
    "build": "dotenv -e .env.local -- turbo run build",
    "clean": "turbo run clean",
    "dependencies:check": "madge --circular --extensions ts,tsx,js,jsx --ts-config ./tsconfig.json ./packages",
    "dependencies:fix": "madge --circular --extensions ts,tsx,js,jsx --ts-config ./tsconfig.json ./packages --fix",
    "dependencies:graph": "madge --circular --extensions ts,tsx,js,jsx --ts-config ./tsconfig.json ./packages --image graph.svg",
    "dependencies:stats": "madge --circular --extensions ts,tsx,js,jsx --ts-config ./tsconfig.json ./packages --stats",
    "dependencies:unused:fix": "madge --circular --extensions ts,tsx,js,jsx --ts-config ./tsconfig.json ./packages --warning --fix",
    "dependencies:unused": "madge --circular --extensions ts,tsx,js,jsx --ts-config ./tsconfig.json ./packages --warning",
    "dev": "dotenv -e .env.local -- turbo dev --concurrency=12 --no-cache --continue",
    "lint-staged": "lint-staged",
    "lint": "turbo run lint",
    "list:projects": "pnpm m ls --depth -1 --json",
    "list:projects:dependencies": "pnpm recursive list --only-projects",
    "packages:versions:check": "npx npm-check-updates -u --deep",
    "packages:versions:update": "npx npm-check-updates -u --deep",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "prettier:check": "prettier --check --ignore-unknown .",
    "prettier:fix": "prettier --write --ignore-unknown .",
    "prettier:list-different": "npx prettier . --list-different",
    "shadcn:add": "pnpm dlx shadcn-ui@latest add --cwd packages/@uncnsrdlabel/components --path atoms",
    "start": "dotenv -e .env.local -- turbo run start",
    "test:e2e": "playwright test",
    "test": "echo \"Error: no test specified\" && exit 0",
    "types:check": "turbo run types:check",
    "unused:fix": "npx next-unused --fix",
    "unused": "npx next-unused",
    "update": "turbo run update",
    "validate": "pnpm test && pnpm types:check && pnpm build && pnpm unused"
  }
}
