  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write --ignore-unknown"
    ],
    "**/*.{md,mdx,json}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "browserslist": [
    "defaults"
  ],
  "next-unused": {
    "alias": {
      "#/lib/*": [
        "lib/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  },
  "resolutions": {
    "@types/react": "18.0.28"
  },
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "generate": "pnpm -r run generate",
    "generate:watch": "pnpm -r run generate:watch",
    "dev": "run-p dev:next generate:watch",
    "dev:next": "next dev",
    "dev:next:inspect": "NODE_OPTIONS='--inspect' next dev",
    "find:unused": "npx next-unused",
    "lint": "next lint",
    "lint-staged": "lint-staged && pnpm deps:check",
    "postbuild": "next-sitemap",
    "prettier:check": "prettier --check --ignore-unknown .",
    "prettier:fix": "prettier --write --ignore-unknown .",
    "prettier:list-different": "npx prettier . --list-different",
    "start": "next start",
    "test": "npm prettier:check && npm lint",
    "typecheck": "tsc --noEmit",
    "deps:check": "madge --exclude '^(@types|lib|.vercel|.next|.idea)' --circular --extensions ts,tsx,js,jsx .",
    "storybook": "pnpm generate && start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "storybook:build:preview": "npx http-server ./storybook-static",
    "prepare": "npx husky install"
  },
  "packageManager": "pnpm@7.29.1"
}
