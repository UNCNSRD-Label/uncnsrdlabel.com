{
  "plugins": [
    "postcss-import",
    [
      "tailwindcss/nesting",
      {
        "postcssPlugin": "postcss-nesting"
      }
    ],
    "tailwindcss",
    [
      "postcss-preset-env",
      {
        "features": { "nesting-rules": false }
      }
    ],
    "autoprefixer",
    "cssnano"
  ]
}
