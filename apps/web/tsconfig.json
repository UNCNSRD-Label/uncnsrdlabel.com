{
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "types": [
      "@types/gtag.js",
      "@types/lodash",
      "@types/node",
      "@types/react-dom",
      "@types/react",
      "@types/use-analytics",
      "@uncnsrdlabel/types"
    ],
    "allowJs": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.json",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "extends": "@uncnsrdlabel/tsconfig/nextjs.json",
  "exclude": [
    "node_modules"
  ]
}
