{
  "$schema": "https://turbo.build/schema.json",
  "globalDotEnv": [".env", ".env.local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "dotEnv": [
        ".env.production.local",
        ".env.local",
        ".env.production",
        ".env"
      ],
      "env": [],
      "outputs": ["!.next/cache/**", ".next/**", "dist/**"]
    },
    "@uncnsrdlabel/app-web#build": {
      "dependsOn": ["^build"],
      "outputs": ["!.next/cache/**", ".next/**", "dist/**"],
      "env": [
        "NEXT_PUBLIC_GOOGLE_TAG_MANAGER_ID",
        "NEXT_PUBLIC_HYGRAPH_API_TOKEN",
        "NEXT_PUBLIC_HYGRAPH_PROJECT_ID",
        "NEXT_PUBLIC_HYGRAPH_REGION",
        "NEXT_PUBLIC_SHOPIFY_STOREFRONT_ACCESS_TOKEN",
        "NEXT_PUBLIC_SHOPIFY_STORE_DOMAIN",
        "NEXT_PUBLIC_SHOPIFY_STOREFRONT_API_VERSION",
        "NEXT_PUBLIC_SUPABASE_ANON_KEY",
        "NEXT_PUBLIC_SUPABASE_URL",
        "SITE_NAME",
        "SITE_URL",
        "SUPABASE_SERVICE_ROLE_KEY"
      ]
    },
    "@uncnsrdlabel/graphql-hygraph#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "env": [
        "NEXT_PUBLIC_HYGRAPH_API_TOKEN",
        "NEXT_PUBLIC_HYGRAPH_PROJECT_ID",
        "NEXT_PUBLIC_HYGRAPH_REGION"
      ]
    },
    "@uncnsrdlabel/graphql-shopify-storefront#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "env": [
        "NEXT_PUBLIC_SHOPIFY_SHOP_ID",
        "NEXT_PUBLIC_SHOPIFY_STOREFRONT_ACCESS_TOKEN",
        "NEXT_PUBLIC_SHOPIFY_STOREFRONT_API_VERSION",
        "NEXT_PUBLIC_SHOPIFY_STORE_DOMAIN",
        "SHOPIFY_PRIVATE_ACCESS_TOKEN",
        "SHOPIFY_STORE_SUBDOMAIN"
      ]
    },
    "@uncnsrdlabel/providers#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "env": [
        "NEXT_PUBLIC_SHOPIFY_STOREFRONT_ACCESS_TOKEN",
        "NEXT_PUBLIC_SHOPIFY_STORE_DOMAIN",
        "NEXT_PUBLIC_SHOPIFY_STOREFRONT_API_VERSION"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dotEnv": [
        ".env.development.local",
        ".env.local",
        ".env.development",
        ".env"
      ],
      "env": [
        "NEXT_PUBLIC_GOOGLE_TAG_MANAGER_ID",
        "NEXT_PUBLIC_HYGRAPH_API_TOKEN",
        "NEXT_PUBLIC_HYGRAPH_PROJECT_ID",
        "NEXT_PUBLIC_HYGRAPH_REGION",
        "NEXT_PUBLIC_SHOPIFY_STORE_DOMAIN",
        "NEXT_PUBLIC_SHOPIFY_STOREFRONT_ACCESS_TOKEN",
        "NEXT_PUBLIC_SHOPIFY_STOREFRONT_API_VERSION",
        "NEXT_PUBLIC_SUPABASE_ANON_KEY",
        "NEXT_PUBLIC_SUPABASE_URL",
        "SITE_NAME",
        "SITE_URL",
        "SUPABASE_SERVICE_ROLE_KEY"
      ]
    },
    "generate-types": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "start": {
      "cache": false,
      "dependsOn": ["^build"],
      "outputs": []
    },
    "clean": {
      "cache": false
    },
    "types:check": {
      "outputs": []
    }
  }
}
