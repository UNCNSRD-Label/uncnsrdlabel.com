{
  "dependencies": {
    "@graphql-codegen/cli": "latest",
    "@graphql-codegen/client-preset": "latest",
    "@graphql-typed-document-node/core": "latest",
    "@shopify/hydrogen-react": "^2023.7.0",
    "@tanstack/react-query": "^4.32.6",
    "@uncnsrdlabel/lib": "workspace:*",
    "dotenv-cli": "latest",
    "graphql": "latest",
    "graphql-config": "latest",
    "graphql-request": "latest",
    "graphql-tag": "latest",
    "lodash": "latest",
    "next": "13.4.13"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.196",
    "@types/node": "latest",
    "@uncnsrdlabel/eslint-config-custom": "workspace:*",
    "@uncnsrdlabel/tsconfig": "workspace:*",
    "@uncnsrdlabel/types": "workspace:*",
    "prettier": "latest",
    "tsup": "latest",
    "type-fest": "latest",
    "typescript": "latest",
    "typescript-graphql-request": "latest"
  },
  "exports": {
    "./*": "./dist/*"
  },
  "files": [
    "src/**"
  ],
  "main": "./src/index.tsx",
  "name": "@uncnsrdlabel/graphql-shopify-storefront",
  "publishConfig": {
    "directory": "src"
  },
  "scripts": {
    "build_old": "graphql-codegen && tsc --project ./tsconfig.json",
    "dev_old": "graphql-codegen --watch && tsc --project ./tsconfig.json --watch",
    "build": "graphql-codegen",
    "lint": "eslint \"**/*.ts*\"",
    "types:check": "tsc --noEmit"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "type": "module",
  "types": "./dist/index.d.ts",
  "version": "0.0.0"
}
