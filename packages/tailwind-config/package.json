{
  "dependencies": {
    "tailwindcss": "^3.3.3"
  },
  "devDependencies": {
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.5",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^20.5.6",
    "@uncnsrdlabel/eslint-config-custom": "workspace:*",
    "@uncnsrdlabel/tsconfig": "workspace:*",
    "@uncnsrdlabel/types": "workspace:*",
    "prettier": "^3.0.2",
    "tsup": "^7.2.0",
    "type-fest": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "exports": {
    ".": "./src/index.ts",
    "./*": [
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*/index.ts"
    ]
  },
  "files": [
    "src"
  ],
  "main": "./src/index.js",
  "module": "./src/index.mjs",
  "name": "@uncnsrdlabel/tailwind-config",
  "publishConfig": {
    "directory": "dist"
  },
  "scripts": {
    "build": "tsc --project ./tsconfig.json",
    "dev": "tsc --project ./tsconfig.json --watch",
    "generate:component": "turbo gen react-component",
    "lint": "eslint \"**/*.ts*\"",
    "types:check": "tsc --noEmit"
  },
  "sideEffects": false,
  "type": "module",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "index.d.ts": ["dist/index.d.ts"]
    }
  },
  "version": "0.0.0"
}
