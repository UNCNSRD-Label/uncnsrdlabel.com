{
  "devDependencies": {
    "@shopify/hydrogen": "^2023.7.3",
    "@types/node": "^20.5.6",
    "@uncnsrdlabel/eslint-config-custom": "workspace:*",
    "@uncnsrdlabel/tsconfig": "workspace:*",
    "prettier": "^3.0.2",
    "tsup": "^7.2.0",
    "type-fest": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "exports": {
    ".": "./dist/index.ts",
    "./*": [
      "./dist/*.ts",
      "./dist/*.tsx",
      "./dist/*/index.ts"
    ]
  },
  "files": [
    "dist"
  ],
  "main": "./src/index.js",
  "module": "./src/index.mjs",
  "name": "@uncnsrdlabel/types",
  "scripts": {
    "build": "tsc --project ./tsconfig.json",
    "dev": "tsc --project ./tsconfig.json --watch",
    "lint": "eslint \"**/*.ts*\"",
    "types:check": "tsc --noEmit"
  },
  "sideEffects": false,
  "type": "module",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "index.d.ts": ["dist/index.d.ts"]
    }
  },
  "version": "0.0.0"
}
