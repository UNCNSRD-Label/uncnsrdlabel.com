{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "incremental": true,
    "jsx": "preserve",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "ESNext",
    "noEmit": true,
    // "paths": {
    //   "@/*": ["../apps/web/src/*"],
    //   "@uncnsrdlabel/app-web/*": ["../apps/web/src/*"],
    //   "@uncnsrdlabel/graphql-hygraph/*": ["../../packages/graphql-hygraph/src/*"],
    //   "@uncnsrdlabel/graphql-shopify-admin/*": ["../../packages/graphql-shopify-admin/src/*"],
    //   "@uncnsrdlabel/graphql-shopify-storefront/*": ["../../packages/graphql-shopify-storefront/src/*"],
    //   "@uncnsrdlabel/lib/*": ["../../packages/lib/src/*"],
    //   "@uncnsrdlabel/providers/*": ["../../packages/providers/src/*"],
    //   "@uncnsrdlabel/tailwind-config/*": ["../../packages/tailwind-config/src/*"],
    //   "@uncnsrdlabel/types/*": ["../../packages/types/src/*"],
    //   "@uncnsrdlabel/ui/*": ["../../packages/ui/src/*"]
    // },
    "plugins": [{ "name": "next" }],
    "strict": false,
    "target": "es5",
    "types": [
      "gtag.js",
      "lodash",
      "node",
      "react-dom",
      "react",
      "use-analytics",
      "@uncnsrdlabel/types"
    ]
  },
  "display": "Next.js",
  "exclude": ["node_modules"],
  "extends": "./base.json",
  "include": ["src", "next-env.d.ts"]
}