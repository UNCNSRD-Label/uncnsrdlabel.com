{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "incremental": true,
    "jsx": "preserve",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "ESNext",
    "noEmit": true,
    "plugins": [{ "name": "next" }],
    "strict": false,
    "target": "es5",
    "types": [
      "gtag.js",
      "lodash",
      "node",
      "react-dom",
      "react",
      "use-analytics",
      "@uncnsrdlabel/types"
    ]
  },
  "display": "Next.js",
  "exclude": ["node_modules"],
  "extends": "./base.json",
  "include": ["src", "next-env.d.ts"]
}