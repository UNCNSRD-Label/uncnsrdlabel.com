{
  "dependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.4",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@shopify/hydrogen-react": "^2024.1.0",
    "@tanstack/react-query": "^5.22.2",
    "@types/node": "^20.5.6",
    "@uncnsrdlabel/lib": "workspace:*",
    "graphql": "^16.8.1",
    "graphql-config": "^5.0.3",
    "graphql-request": "^6.1.0",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "react": "experimental",
    "slugify": "^1.6.6",
    "xo": "^0.57.0",
    "zod": "^3.22.4",
    "zod-fetch": "^0.1.1"
  },
  "devDependencies": {
    "@graphql-eslint/eslint-plugin": "^3.20.1",
    "@types/lodash": "^4.14.202",
    "@types/node": "latest",
    "@types/react": "18.2.57",
    "@uncnsrdlabel/tsconfig": "workspace:*",
    "@uncnsrdlabel/types": "workspace:*",
    "prettier": "^3.2.5",
    "type-fest": "^4.10.3",
    "typescript": "^5.3.3",
    "typescript-graphql-request": "^4.4.6"
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js"
  },
  "files": [
    "dist/**"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "name": "@uncnsrdlabel/graphql-shopify-storefront",
  "peerDependencies": {
    "@parcel/watcher": "^2.2.0"
  },
  "scripts": {
    "build": "graphql-codegen && tsc --project ./tsconfig.json",
    "clean": "rm -rf dist",
    "dev:graphql-codegen": "graphql-codegen --watch",
    "dev:ts": "tsc --project ./tsconfig.json --watch",
    "dev": "npm-run-all dev:graphql-codegen dev:ts",
    "generate:component": "turbo gen react-component",
    "lint": "xo",
    "lint:fix": "xo --fix",
    "types:check": "tsc --noEmit"
  },
  "sideEffects": false,
  "type": "module",
  "types": "./dist/index.d.ts",
  "version": "0.0.0"
}
