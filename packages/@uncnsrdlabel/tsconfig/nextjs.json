{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "declaration": false,
    "declarationMap": false,
    "incremental": true,
    "jsx": "preserve",
    "lib": ["dom", "dom.iterable", "ESNext", "webworker"],
    "noEmit": true,
    "plugins": [{ "name": "next" }],
    "resolveJsonModule": true,
    "target": "ESNext"
  },
  "display": "Next.js",
  "exclude": ["node_modules"],
  "extends": "./base.json",
  "include": ["@uncnsrdlabel/types", "next-env.d.ts", "src"]
}
