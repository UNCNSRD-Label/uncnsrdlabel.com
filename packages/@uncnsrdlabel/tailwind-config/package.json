{
  "dependencies": {
    "tailwindcss": "^3.4.1",
    "xo": "^0.57.0"
  },
  "devDependencies": {
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.11.20",
    "@uncnsrdlabel/tsconfig": "workspace:*",
    "@uncnsrdlabel/types": "workspace:*",
    "prettier": "^3.2.5",
    "tailwindcss-animate": "^1.0.7",
    "type-fest": "^4.10.3",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist/**"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "name": "@uncnsrdlabel/tailwind-config",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc --project ./tsconfig.json",
    "clean": "rm -rf dist",
    "dev": "tsc --project ./tsconfig.json --watch",
    "generate:component": "turbo gen react-component",
    "lint": "xo",
    "lint:fix": "xo --fix",
    "types:check": "tsc --noEmit"
  },
  "sideEffects": false,
  "type": "module",
  "types": "./dist/index.d.ts",
  "version": "0.0.0"
}
