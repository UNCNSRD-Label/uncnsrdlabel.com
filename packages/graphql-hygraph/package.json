{
  "dependencies": {
    "@graphql-codegen/cli": "latest",
    "@graphql-codegen/client-preset": "latest",
    "@graphql-typed-document-node/core": "latest",
    "@types/node": "latest",
    "@uncnsrdlabel/lib": "workspace:*",
    "dotenv-cli": "latest",
    "graphql-config": "latest",
    "graphql-request": "latest",
    "graphql-tag": "latest",
    "graphql": "^16.7.1",
    "slugify": "^1.6.6",
    "zod-fetch": "^0.1.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@uncnsrdlabel/eslint-config-custom": "workspace:*",
    "@uncnsrdlabel/tsconfig": "workspace:*",
    "@uncnsrdlabel/types": "workspace:*",
    "eslint": "^8.46.0",
    "prettier": "latest",
    "tsup": "latest",
    "type-fest": "latest",
    "typescript-graphql-request": "latest",
    "typescript": "latest"
  },
  "files": [
    "dist/**"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "name": "@uncnsrdlabel/graphql-hygraph",
  "peerDependencies": {
    "@parcel/watcher": "^2.2.0"
  },
  "scripts": {
    "build": "graphql-codegen && tsup",
    "clean": "rm -rf dist",
    "dev": "graphql-codegen --watch && tsup --watch",
    "generate:component": "turbo gen react-component",
    "lint": "eslint \"**/*.ts*\"",
    "types:check": "tsc --noEmit"
  },
  "sideEffects": false,
  "types": "./dist/index.d.ts",
  "version": "0.0.0"
}
