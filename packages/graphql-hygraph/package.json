{
  "dependencies": {
    "@graphql-codegen/cli": "latest",
    "@graphql-codegen/client-preset": "latest",
    "@graphql-typed-document-node/core": "latest",
    "@types/node": "latest",
    "@uncnsrdlabel/tsconfig": "workspace:*",
    "dotenv-cli": "latest",
    "graphql": "^16.7.1",
    "graphql-config": "latest",
    "graphql-request": "latest",
    "graphql-tag": "latest"
  },
  "devDependencies": {
    "@uncnsrdlabel/eslint-config-custom": "workspace:*",
    "@uncnsrdlabel/tsconfig": "workspace:*",
    "tsup": "latest",
    "type-fest": "latest",
    "typescript": "latest",
    "prettier": "latest",
    "typescript-graphql-request": "latest"
  },
  "exports": {
    ".": {
    "require": "./dist/index.js",
    "import": "./dist/index.js",
    "default": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "./*": {
    "require": "./dist/*.js",
    "import": "./dist/*.js",
    "types": "./dist/*.d.ts"
  }
  },
  "files": ["dist/**"],
  "license": "MIT",
"publishConfig": {
  "directory": "dist"
},
  "name": "@uncnsrdlabel/graphql-hygraph",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint \"**/*.ts*\"",
    "types:check": "tsc --noEmit"
  },
  "type": "module",
  "types": "./dist/index.d.ts"
}
