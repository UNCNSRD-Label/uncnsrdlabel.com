{
  "name": "@collagerie/graphql-shopify",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "release": "taskr release",
    "build": "taskr build",
    "dev": "taskr",
    "types": "tsc --emitDeclarationOnly",
    "prettier-fix": "prettier --write .",
    "generate": "DOTENV_CONFIG_PATH=./.env graphql-codegen -r dotenv/config",
    "generate:watch": "npm run generate -- --watch"
  },
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js",
    "./*": [
      "./dist/*.js",
      "./dist/*/index.js"
    ],
    "./next.config": "./dist/next.config.cjs"
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ],
      "next.config": [
        "dist/next.config.d.cts"
      ]
    }
  },
  "files": [
    "generated"
  ],
  "dependencies": {
    "@graphql-typed-document-node/core": "^3.1.1",
    "@shopify/hydrogen": "^1.6.5",
    "@shopify/hydrogen-react": "2022.10.8",
    "graphql-config": "^4.4.0"
  },
  "peerDependencies": {
    "react": "^18"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.16.4",
    "@graphql-codegen/client-preset": "1.2.6",
    "@taskr/clear": "^1.1.0",
    "@taskr/esnext": "^1.1.0",
    "@taskr/watch": "^1.1.0",
    "@types/node": "^18.11.18",
    "dotenv": "^16.0.3",
    "graphql": "^16.6.0",
    "lint-staged": "^13.1.0",
    "prettier": "latest",
    "taskr": "^1.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "resolutions": {
    "graphql": "16.6.0"
  }
}
